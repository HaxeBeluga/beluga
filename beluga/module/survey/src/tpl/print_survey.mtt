<div id="result">
	<h1>Title of Survey</h1>
	<table>
		<thead>::title::</thead>
		<tbody>
			::foreach choices::
			<tr>
				<td>::name::</td>
				<td class="bar">
					<div style="width: ::pourcent::%;"></div>
				</td>
				<td>::vote:: (::pourcent:: %)</td>
			</tr>
			::end::
		</tbody>
	</table>
</div>